@model Parking.Models.PriceTable

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes da Tabela de Preço</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.InicioVigencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InicioVigencia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FimVigencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FimVigencia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ValorHoraInical)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ValorHoraInical)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ValorHoraAdicional)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ValorHoraAdicional)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">
        <input class="btn btn-success" type="Submit" value="Editar">
    </a>
    <a asp-action="Index">
        <input class="btn btn-success" type="Submit" value="Voltar">
    </a>
</div>
